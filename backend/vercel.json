{
    "version": 2,
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/src/(.*)",
        "dest": "/src/$1"
      },
      {
        "src": "/public/images/(.*)",
        "dest": "/public/images/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  